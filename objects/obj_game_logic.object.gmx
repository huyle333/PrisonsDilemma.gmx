            <string>var round_end = htme_globalGet("round_end");

if (round_end &amp;&amp; obj_player.is_host)
{
    var playerlist = htme_getPlayers();
    var playerlist_size = ds_list_size(playerlist);
    var split = 0;
    var steal = 0;
    
    for (var i = 0; i&lt;playerlist_size; i++)
    {
        var player = ds_list_find_value(playerlist, i);
        var instance = htme_findPlayerInstance(obj_player, player);
        
        if (instance != -1)
        {
            choices[i] = (instance).choice;
            if (choices[i] == "Split")
                split++;
            else
                steal++;    
        }
    }
 
    for (var i = 0; i&lt;playerlist_size; i++)
    {
        var player = ds_list_find_value(playerlist, i);
        var instance = htme_findPlayerInstance(obj_player, player);
        
        if (instance != -1)
        {

            // RRGG
            if (split == steal)
                continue;
            
            // GGGG
            if (split == 4)
                (instance).points += 100;
 
            // RRRR
            if (steal == 4)
                (instance).points -= 150;
                
            //RGGG
            if (choices[i] == "Steal" &amp;&amp; steal == 1)
                (instance).points += 500;
            else if (choices[i] == "Split" &amp;&amp; steal == 1)
                continue;
                
            //GRRR
            if (choices[i] == "Split" &amp;&amp; split == 1)
                (instance).points -= 500;
            else if (choices[i] == "Steal" &amp;&amp; split == 1)
                continue;
        }
    }    
}

htme_globalSet("round_end", false, buffer_bool);
obj_ctrl_round.round_start = false;
</string>
