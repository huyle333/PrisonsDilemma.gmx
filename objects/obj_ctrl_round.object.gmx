        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (obj_player.is_host)
{

var ready_counter = 0;

var playerlist = htme_getPlayers();
var playerlist_size = ds_list_size(playerlist);

for (var i = 0; i&lt;playerlist_size; i++)
{
    var player = ds_list_find_value(playerlist, i);
    var instance = htme_findPlayerInstance(obj_player, player);
    
    if (instance != -1)
    {
        var ready = (instance).ready;
        
        if (ready)
        {
            ready_counter++;
        }
    }

}



if (ready_counter == self.group_size &amp;&amp; self.round_start == false)
{
    self.round_start = true;
   // htme_globalSet("round_start", self.round_start, buffer_bool);

    //var round_end = htme_globalGet("round_end");

    var playerlist = htme_getPlayers();
    var playerlist_size = ds_list_size(playerlist);
    var split = 0;
    var steal = 0;
    
    for (var i = 0; i&lt;playerlist_size; i++)
    {
        var player = ds_list_find_value(playerlist, i);
        var instance = htme_findPlayerInstance(obj_player, player);
        
        if (instance != -1)
        {
            choices[i] = (instance).choice;
            show_debug_message((instance).name + " - " + (instance).choice);
            if (choices[i] == "Split")
                split++;
            else
                steal++;    
        }
    }
 
    for (var i = 0; i&lt;playerlist_size; i++)
    {
        var player = ds_list_find_value(playerlist, i);
        var instance = htme_findPlayerInstance(obj_player, player);
        
        if (instance != -1)
        {

            // RRGG
            if (split == steal)
                continue;
            
            // GGGG
            if (split == 4)
                (instance).points += 100;
 
            // RRRR
            if (steal == 4)
                (instance).points -= 150;
                
            //RGGG
            if (choices[i] == "Steal" &amp;&amp; steal == 1)
                (instance).points += 500;
            else if (choices[i] == "Split" &amp;&amp; steal == 1)
                continue;
                
            //GRRR
            if (choices[i] == "Split" &amp;&amp; split == 1)
                (instance).points -= 500;
            else if (choices[i] == "Steal" &amp;&amp; split == 1)
                continue;
        }
    }    

        
    
    //htme_globalSet("round_end", true, buffer_bool);
    
}

}

</string>
